{
	"name": "owservable",
	"version": "2.0.1",
	"description": "owservable backend",
	"engines": {
		"node": ">=20"
	},
	"main": "./lib/owservable.js",
	"types": "./lib/owservable.d.ts",
	"author": "Predrag Stojadinovic <predrag@stojadinovic.net>",
	"license": "Unlicense",
	"files": [
		"lib/**/*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/owservable/owservable.git"
	},
	"scripts": {
		"all": "yarn & yarn clean & yarn format & yarn build & yarn test & yarn docs & skeletest & strictr",
		"clear:lib": "del /S /Q lib\\*",
		"build": "yarn clear:lib & tsc",
		"build:watch": "tsc --watch",
		"lint": "tslint -p tsconfig.json",
		"clean": "tslint --config tslint-imports.json --fix --project .",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"docs": "typedoc --readme none --out docs ./src/owservable.ts",
		"sync": "git pull & git fetch origin & git merge origin/main & yarn & git branch",
		"test": "jest --coverage --config jest.config.js",
		"quick": "jest --config jest.config.js",
		"test:watch": "jest --watch --config jest.config.js",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"sonar": "sonarjs -e .sonarlint/**,node_modules/**,coverage/**",
		"test_qube": "jest --coverage --testResultsProcessor=jest-sonar-reporter --config jest.config.js",
		"up": "yarn upgrade-interactive",
		"latest": "yarn upgrade-interactive --latest",
		"l": "yarn & yarn latest",
		"audit-fix": "npx --yes yarn-audit-fix",
		"srctree": "dir src /s /b > srctree.txt"
	},
	"dependencies": {
		"@owservable/actions": "^2.0.1",
		"@owservable/folders": "^2.0.1",
		"@types/lodash": "^4.17.20",
		"@types/node": "^25.0.3",
		"@types/node-cron": "^3.0.11",
		"deprecated-decorator": "^0.1.6",
		"json-circular-stringify": "^1.0.1",
		"jsondiffpatch": "^0.5.0",
		"lodash": "^4.17.20",
		"mongoose": "^8.19.4",
		"node-cron": "^4.2.1",
		"rxjs": "^7.8.2",
		"sift": "^17.1.3"
	},
	"devDependencies": {
		"@types/jest": "^30.0.0",
		"@types/rewire": "^2.5.30",
		"coveralls": "^3.0.9",
		"jest": "^30.2.0",
		"jest-sonar-reporter": "^2.0.0",
		"prettier": "^3.7.4",
		"rewire": "^9.0.1",
		"skeletest": "^0.7.3",
		"sonarjs": "^1.0.0",
		"source-map-support": "^0.5.21",
		"strictr": "^0.5.5",
		"ts-jest": "^29.4.6",
		"ts-node": "^10.9.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"tslint-etc": "^1.13.7",
		"typedoc": "^0.28.15",
		"typescript": "^5.9.3"
	},
	"packageManager": "yarn@1.22.22"
}
